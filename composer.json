{
    "name": "mcp/mcp-sdk-php",
    "description": "Model Context Protocol (MCP) is an open protocol that standardizes how applications provide context to LLMs.",
    "keywords": [
        "Model Context Protocol",
        "MCP",
        "MCP Server",
        "MCP Client",
        "MCP SDK PHP"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/mcp-sdk-php",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "psr/log": "^3.0",
        "cboden/ratchet": "^0.4",
        "monolog/monolog": "^3.0"
    },
    "suggest": {
        "ext-pcntl": "For better process handling in CLI environments"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Mcp\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
